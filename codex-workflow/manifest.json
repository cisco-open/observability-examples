  "dependencies": [
    "fmm",
    "codex",
    "k8s",
    "apm"
  ],
  "description": "Health violation counter",
  "contact": "abokhand@cisco.com",
  "homepage": "https://github.com/geoffhendrey/cop-examples/tree/main/example/codex-workflow",
